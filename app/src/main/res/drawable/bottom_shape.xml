<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape>
        <solid android:color="#86CD88"/>
        <corners android:topRightRadius="15dp"
            android:topLeftRadius="15dp"/>
        <stroke
            android:width="2dp"
            android:color="@color/white"/>
    </shape>

</item>
    <item
         android:right="2dp"
        android:left="2dp"
        android:top="4dp"
        >
        <shape>
            <solid android:color="#7485E3"/>
            <corners android:topRightRadius="15dp"
                android:topLeftRadius="15dp"/>
        </shape>

    </item>
</layer-list>